{
  "extends": "airbnb-base",
  "plugins": ["prettier"],
  "env": {
    "browser": true
  },
  "rules": {
    "arrow-parens": "off",
    "class-methods-use-this": "off",
    "import/prefer-default-export": "off",
    "object-curly-newline": "off",
    "prettier/prettier": "error",
    "quotes": "off"
  }
}
